{
  "name": "shops-vendure-backend",
  "version": "1.4.2",
  "private": true,
  "engines": {
    "yarn": "Please use NPM",
    "npm": ">=10"
  },
  "scripts": {
    "start": "node ./dist/index",
    "start:dev": "ts-node test/start-dev-server.ts",
    "serve": "nodemon -e ts --watch './src/' --exec \"npm run build build && node ./dist/index\"",
    "serve:prod": "export ENV_FILE=.env && npm run serve",
    "serve:test": "export ENV_FILE=.env.test && npm run serve",
    "serve:wkw-test": "export ENV_FILE=.env.wkw-test && npm run serve",
    "serve:wkw-prod": "export ENV_FILE=.env.wkw-prod && npm run serve",
    "script:prod": "export ENV_FILE=.env && ts-node",
    "script:test": "export ENV_FILE=.env.test && ts-node",
    "script:wkw-test": "export ENV_FILE=.env.wkw-test && ts-node",
    "script:wkw-prod": "export ENV_FILE=.env.wkw-prod && ts-node",
    "copy": "copyfiles -u 1 'src/__admin-ui/dist/**/*' dist",
    "build": "tsc && npm run copy",
    "build:admin": "rimraf src/__admin-ui && ts-node src/compile-admin-ui.ts",
    "generate-migration": "ts-node migration generate",
    "generate-migration:test": "export ENV_FILE=.env.test && npm run generate-migration",
    "generate-migration:prod": "export ENV_FILE=.env && npm run generate-migration",
    "generate-migration:wkw-test": "export ENV_FILE=.env.wkw-test && npm run generate-migration",
    "generate-migration:wkw-prod": "export ENV_FILE=.env.wkw-prod && npm run generate-migration",
    "run-migration": "ts-node migration run",
    "run-migration:test": "export ENV_FILE=.env.test && npm run run-migration",
    "run-migration:prod": "export ENV_FILE=.env && npm run run-migration",
    "run-migration:wkw-test": "export ENV_FILE=.env.wkw-test && npm run run-migration",
    "run-migration:wkw-prod": "export ENV_FILE=.env.wkw-prod && npm run run-migration",
    "revert-migration": "ts-node migration revert",
    "rever-migration:test": "export ENV_FILE=.env.test && npm run revert-migration",
    "revert-migration:prod": "export ENV_FILE=.env && npm run revert-migration",
    "revert-migration:wkw-test": "export ENV_FILE=.env.wkw-test && npm run revert-migration",
    "revert-migration:wkw-prod": "export ENV_FILE=.env.wkw-prod && npm run revert-migration"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "6.0.0",
    "@google-cloud/storage": "7.11.2",
    "@mollie/api-client": "3.7.0",
    "@pinelab/vendure-plugin-admin-ui-helpers": "1.2.0",
    "@pinelab/vendure-plugin-coinbase": "1.2.0",
    "@pinelab/vendure-plugin-dutch-postalcode": "1.2.0",
    "@pinelab/vendure-plugin-e-boekhouden": "1.2.0",
    "@pinelab/vendure-plugin-goedgepickt": "1.2.0",
    "@pinelab/vendure-plugin-google-cloud-tasks": "1.6.0",
    "@pinelab/vendure-plugin-google-storage-assets": "1.2.0",
    "@pinelab/vendure-plugin-invoices": "2.4.0",
    "@pinelab/vendure-plugin-limit-variant-per-order": "2.1.0",
    "@pinelab/vendure-plugin-metrics": "1.3.0",
    "@pinelab/vendure-plugin-myparcel": "1.3.0",
    "@pinelab/vendure-plugin-order-export": "1.2.0",
    "@pinelab/vendure-plugin-popularity-scores": "^1.7.0",
    "@pinelab/vendure-plugin-selectable-gifts": "1.2.0",
    "@pinelab/vendure-plugin-sendcloud": "1.3.0",
    "@pinelab/vendure-plugin-shipping-by-weight-and-country": "1.1.1",
    "@pinelab/vendure-plugin-stock-monitoring": "1.4.0",
    "@pinelab/vendure-plugin-variant-bulk-update": "1.2.0",
    "@pinelab/vendure-plugin-webhook": "1.2.1",
    "@pinelab/vendure-scripts": "^1.3.0",
    "@types/adm-zip": "0.5.5",
    "@vendure/admin-ui-plugin": "2.2.6",
    "@vendure/asset-server-plugin": "2.2.6",
    "@vendure/core": "2.2.6",
    "@vendure/email-plugin": "2.2.6",
    "@vendure/migrate-v2": "^0.4.0",
    "@vendure/payments-plugin": "2.2.6",
    "@vendure/ui-devkit": "2.2.6",
    "adm-zip": "0.5.14",
    "copyfiles": "2.4.1",
    "mysql": "2.18.1",
    "sharp": "^0.33.4",
    "tmp": "0.2.3",
    "typescript": "5.3.3",
    "winston": "3.11.0"
  },
  "devDependencies": {
    "@types/papaparse": "5.3.14",
    "@types/prompt": "1.1.8",
    "@types/tmp": "0.2.3",
    "@vendure/testing": "2.1.5",
    "localtunnel": "2.0.2",
    "nodemon": "2.0.20",
    "papaparse": "5.4.1",
    "prompt": "1.3.0",
    "ts-node": "10.9.2"
  }
}
